{
    "manifest_version": 3,
    "name": "Tech Stack Finder",
    "version": "1.0",
    "description": "Identify the tech stacks used by websites.",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  